.Offcanvas {
  position: fixed;
  right: 0;
  top: 0;
  height: 100%;
  width: 90%;
  background-color: black;
  transform: scale(1);
  transform-origin: top right;
  transition: transform 250ms ease-in-out;
  z-index: 5;
  padding: 5rem 1rem 0;
}
.Offcanvas.--hidden {
  transform: scale(0);
}
.Offcanvas-Drawer {
  width: 100%;
  height: 100%;
  display: none;
  transition: opacity 250ms;
}
.Offcanvas-Drawer.is-expanded {
  display: flex;
}
