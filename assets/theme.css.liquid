}
.Offcanvas-Drawer-ColLink-container a.--sale::after {
  background-color: #e53e3e !important;
}
.Offcanvas-Drawer-ColLink-container a::after {
  content: "";
  bottom: 0;
  left: 0;
  position: absolute;
  width: 100%;
  height: 2px;
  background-color: white;
  transform: scaleX(0);
  transform-origin: left;
  transition: transform 250ms ease-in-out;
}
