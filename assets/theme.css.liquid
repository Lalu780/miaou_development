.SizesPage {
  background-color: #000000;
  padding: 10rem 1.25rem;
}
.SizesPage-container {
  max-width: 56rem;
  margin: auto;
  background-color: #000000;
  color: #ffffff;
}
.SizesPage-container a {
  border-bottom: 1px solid #333;
  transition: border-color 0.3s ease;
  color: #ffffff;
}
.SizesPage-container a:hover {
  color: #ffffff;
  border-color: #ffffff;
}
.SizesPage-container .overflow-x-scroll {
  overflow-x: scroll;
}
.SizesPage-container h1 {
  font-size: 2.369em;
  margin-bottom: 5rem;
  text-transform: uppercase;
}
.SizesPage-container h6 {
  text-transform: uppercase;
  font-size: 1.1em;
  margin: 2.75rem 0 1.25rem;
}
.SizesPage-container table {
  border-spacing: 0;
}
.SizesPage-container tbody {
  background-color: #000000;
}
.SizesPage-container tbody tr, .SizesPage-container tbodytr:nth-child(even) {
  background-color: #000000 !important;
}
.SizesPage-container tbody tr:first-child td, .SizesPage-container tbodytr:nth-child(even):first-child td {
  border-bottom: 2px solid #ffffff;
}
.SizesPage-container tbody td {
  padding: 1rem 0.25rem;
}
.SizesPage .hide-table {
  display: none;
}
.SizesPage .flex {
  display: flex;
  justify-content: space-between;
  cursor: pointer;
}
.SizesPage .uppercase {
  text-transform: uppercase;
}
.SizesPage select {
  float: right;
  height: 3.125rem;
  max-width: 7.5rem;
}
.SizesPage select, .SizesPage select:hover, .SizesPage select:focus {
  background-color: transparent;
  border-color: #ffffff;
  color: #ffffff;
}
.SizesPage select option {
  color: #000000;
}

.FullBleedVideo {
  height: 100vh;
  padding-top: 4rem;
  background: #000000;
}

