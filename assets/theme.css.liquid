.dropdown-options-1 li {
	margin-right: 30px;
	height: 32px;
	line-height: 44px;
	cursor: pointer;
}

html.mod-no-mobile .dropdown-options-1 li {
	-webkit-transition: opacity .3s ease-in-out 0s;
	transition: opacity .3s ease-in-out 0s;
}

html.mod-no-mobile .dropdown-options-1 li:hover {
	opacity: .4;
}

.dropdown-options-1 li a {
	font-size: 1.1rem;
}

.dropdown-options-1 li a:before {
	content: "";
	display: inline-block;
	margin-right: 8px;
	margin-bottom: 4px;
	width: 20px;
	height: 20px;
	border: 1px solid #000;
	vertical-align: middle;
}

.dropdown-options-1 li.active a:before {
	background-color: #000;
}

.collection-sort:not(.active) {
	display: none;
}
.dropdown-options-2 {
    top: 0;
    right: 0;
    padding: 12px 0 22px;
    border-top: 1px solid #ccc;
    background-color: black;
    color: white;
    list-style: none;
    display: flex;
    flex-direction: column;
  font-size: 13px;
}
 .dropdown-options-2 li {
    text-align: right;
    padding: 12px 20px;
    cursor: pointer;
    height: 26px;
 }

html.mod-no-mobile .dropdown-options-2 li {
	-webkit-transition: opacity .3s ease-in-out 0s;
	transition: opacity .3s ease-in-out 0s;
}

html.mod-no-mobile .dropdown-options-2 li:hover {
	opacity: .4;
} 
/* Collection Filter/Sort CSS End */

