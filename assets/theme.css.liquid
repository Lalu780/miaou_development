  opacity: 0;
  transition: opacity 250ms ease-in-out;
  background-size: auto 80%;
  background-position: center top;
  padding: 0 !important;
  height: 100% !important;
}
.Offcanvas-Drawer-ColLink-container {
  position: relative;
}
.Offcanvas-Drawer-ColLink-container .invisible-toggle-button {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 25px;
  cursor: pointer;
}
.Offcanvas-Drawer-ColLink-container .hidden-3-deep {
  display: none;
  display: flex;
  flex-direction: column;
  height: 0;
  opacity: 0;
  margin-left: 1.5rem;
  transition: 0.5s ease;
}
.Offcanvas-Drawer-ColLink-container .hidden-3-deep.menu-hover-effect {
  height: auto;
  opacity: 1;
}
