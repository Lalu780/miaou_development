    <script>
       $(document).ready(function (e) {
        $('#navigation-menu-toggle').click(function (event) {
        $('#accessibleNav').addClass('is-open');
        $('#accessibleNav').removeClass('is-close');
        $('.navigation-menu').addClass('menu-open');
        $('body').addClass('overflow-hidden');
      });

      $('#hamburger-menu-close').click(function () {
        $('#accessibleNav').addClass('is-close');
        $('#accessibleNav').removeClass('is-open');
        $('.navigation-menu').removeClass('menu-open');
        $('body').removeClass('overflow-hidden');
      });

      $('.menu.menu-bar .menu-link, .mega-menu.mega-menu--multiLevel, .navigation-nosto-section, .universe-banner').mouseenter(function () {
        $('.header.custom-header .nav').addClass('black-bg');
      }).mouseleave(function () {
        $('.header.custom-header .nav').removeClass('black-bg');
      });

      $('.main-sub-nav.site-nav--has-dropdown .mobile-menu-back-item').on('click', function () {
        $(this).parent('.mega-menu.mega-menu--multiLevel').css('left', '100%');
      });
      $('.main-sub-nav.site-nav--has-dropdown .menu-link.menu-bar-link').on('click', function () {
        $(this).next('.mega-menu.mega-menu--multiLevel').css('left', '0');
      }); 
      $('.site-sub-nav--has-submenu-dropdown >a').each(function () {
       $(this).on('touchend click', function () {
        event.stopPropagation();
        event.preventDefault();
        if ($(this).next('ul').hasClass('is_submenu_open')) {
        $('.submenu-sub-dropdown-list').removeClass('is_submenu_open');
        $('a.site-sub-nav--has-dropdown').removeClass('is_submenu_link_open');
        }
        else {
        $('.submenu-sub-dropdown-list').removeClass('is_submenu_open');
         $(this).next('ul').toggleClass('is_submenu_open');
         $('a.site-sub-nav--has-dropdown').removeClass('is_submenu_link_open');
         $(this).toggleClass('is_submenu_link_open');
          }

       })
      });

   var universeFirstOpen = $('.main-sub-nav.site-nav--has-dropdown[aria-label="Universe"] .mega-menu.mega-menu--multiLevel')
    $(universeFirstOpen).each(function () {
     $('.main-sub-nav.site-nav--has-dropdown[aria-label="Universe"] .mega-menu.mega-menu--multiLevel > li:nth-child(4) >ul').addClass('is_submenu_open');
      $('.main-sub-nav.site-nav--has-dropdown[aria-label="Universe"] .mega-menu.mega-menu--multiLevel > li:nth-child(4) >a').addClass('is_submenu_link_open');
        })
                                                          

      var headerFixedScroll = document.querySelector('.header.custom-header .nav');
      var mobileFixedMenu = document.querySelector('.mobile-fixed-menu');
      window.onscroll = function () {
    // pageYOffset or scrollY
        if (window.pageYOffset > 200) {
          headerFixedScroll.classList.add('header-fixed-scroll');
          mobileFixedMenu.classList.add('mobile-fixed-menu-scrolled');
        } else {
          headerFixedScroll.classList.remove('header-fixed-scroll');
          mobileFixedMenu.classList.remove('mobile-fixed-menu-scrolled')
        }
      }
    })
</script>
<script>
  $(document).on('keyup', '#custom-search-bar', function () {
    var searchValue = $(this).val().toLowerCase();
    var collections = $('#popular-collections .collection-item');
    var popularHeading = $('#popular-search-heading');
    console.log('test');
    if (searchValue.length > 0) {
      collections.hide();
      popularHeading.hide();
      collections.filter(function () {
        return $(this).data('title').toLowerCase().indexOf(searchValue) > -1;
      }).show();
    }
  });
</script>
